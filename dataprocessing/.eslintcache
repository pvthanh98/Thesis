                  Tên: {name && name}\r\n               </div>\r\n               <div style={{marginTop:\"8px\"}}>\r\n                 <h3>Thông tin cửa hàng</h3>\r\n                  Tên: {selectedStore && selectedStore.name}\r\n               </div>\r\n                <div style={{marginTop:\"8px\"}}>\r\n                <h3>Đánh Giá</h3>\r\n                <Select\r\n                  labelId=\"demo-simple-select-label\"\r\n                  id=\"demo-simple-select\"\r\n                  value={rating}\r\n                  onChange={handleChange}\r\n                  style={{width:\"100px\"}}\r\n                >\r\n                  <MenuItem value={1}>1</MenuItem>\r\n                  <MenuItem value={2}>2</MenuItem>\r\n                  <MenuItem value={3}>3</MenuItem>\r\n                  <MenuItem value={4}>4</MenuItem>\r\n                  <MenuItem value={5}>5</MenuItem>\r\n                </Select>\r\n               </div>\r\n               <div style={{marginTop:\"8px\",height:\"200px\"}}>\r\n               <h3>Nội dung</h3>\r\n                <TextField\r\n                  id=\"standard-multiline-flexible\"\r\n                  label=\"Multiline\"\r\n                  multiline\r\n                  rowsMax={10}\r\n                  value={comment}\r\n                  style={{width:\"600px\"}}\r\n                  onChange={e=>setComment(e.target.value)}\r\n                />\r\n               </div>\r\n               <div style={{marginTop:\"8px\", height:\"40px\", flexDirection:\"row\", display:\"flex\"}}>\r\n                  <Button onClick={()=>randomRatingValue(-1)} variant=\"contained\">\r\n                    Đánh giá <ArrowDownwardIcon />\r\n                  </Button>\r\n                  <Button style={{marginLeft:\"8px\"}} onClick={()=>randomRatingValue(1)} variant=\"contained\">\r\n                    Đánh giá <ArrowUpwardIcon />\r\n                  </Button>\r\n                  <Button style={{marginLeft:\"8px\"}} color=\"secondary\"  on